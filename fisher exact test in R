dat <- data.frame(
  GWD= c(45,177),
  LWK= c(28, 170),
  row.names= c('star_28','other'),
  stringsAsFactors = FALSE
)
colnames(dat) <- c("GWD", "LWK")

dat

df <- read.csv("C:/Users/rbooy/Documents/test.csv",sep = ',',row.names= c('GWD','LWK'))

#chisq.test(df)$expected

test <- fisher.test(dat)
test

test <- fisher.test(df)
test

install.packages("genetics")
library(genetics)
example.data  <- c("starOther/starOther","starOther/star28","star28/star28)
g1 <- genotype(example.data)
g1
HWE.test(g1)

library(genetics)
two.data   <- c(rep("*1/*1", 346),
                  rep("*1/*28",138),
                  rep("*28/*28",10)
g2  <- genotype(two.data)
g2
HWE.test(g2)
